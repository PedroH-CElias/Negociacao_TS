{
    "compilerOptions": {
        "outDir": "dist/js", // para onde deve ir os arquivos compilados
        "target": "ES6", // linguagem js que deve ser compilada
        "noEmitOnError": true // nao gera arquivo js enquanto ts tiver com erro na compilacao
    },
    "include": [
        "app/**/*" // onde estao os arquivos ts que deve ser compilado
    ]
}